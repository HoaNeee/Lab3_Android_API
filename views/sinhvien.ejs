<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Danh sach sinh vien</h1>
    <Table>
        <thead>
            <tr>
                <th>Ma Sinh vien</th>
                <th>Ten sinh vien</th>
            </tr>
        </thead>
        <tbody>
            <% sinhviens.forEach(function(sinhvien) { %>

           
            <tr>
                <td><%= sinhvien.masv %></td>
                <td><%= sinhvien.tensv %></td>
            </tr> 
            <% }); %>
        </tbody>
    </Table>
</body>
</html>